dir() %>% startsWith(., "week")
library(tidyverse)
dir() %>% filter(startsWith(., "week"))
library(tidyverse)
data.frame(x=dir()) %>% filter(x, startsWith(., "week"))
data.frame(x=dir()) %>% filter(x==startsWith(x, "week"))
data.frame(x=dir())
data.frame(x=dir()) %>% filter(startsWith(x, "week"))
data.frame(x=dir()) %>% filter(startsWith(x, "week"))
library(tidyverse)
Result <- data.frame(x=dir()) %>% filter(startsWith(x, "week"))
Result <- Result %>% mutate(Result = str_replace(x, "-", " "))
Result
library(tidyverse)
Result <- data.frame(x=dir()) %>% filter(startsWith(x, "week"))
Result <- Result %>% mutate(Result = str_replace(x, "-", " ")) %>% mutate(x = paste0('<a href=paste0("https://robertwwalker.github.io/CWD-Slides/",x,"/", sep=""), >',Result, '</a>', sep=""))
Result
library(tidyverse)
library(DT)
Result <- data.frame(x=dir()) %>% filter(startsWith(x, "week"))
Result <- Result %>% mutate(Result = str_replace(x, "-", " ")) %>% mutate(x = paste0('<a href=',paste0("https://robertwwalker.github.io/CWD-Slides/",x,"/", sep=""),' >',Result, '</a>', sep=""))
datatable(Result, escape = FALSE)
